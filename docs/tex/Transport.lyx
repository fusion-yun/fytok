\begin_inset Formula 
\begin{equation}
\mathbf{j}=\frac{1}{\mu_{0}}\left(-\frac{\left(2\pi\right)^{e_{Bp}}}{\sigma_{Bp}}F^{\prime}\mathbf{B}+\left[\left(2\pi\right)^{e_{Bp}}FF^{\prime}+\frac{\sigma_{Bp}}{\left(2\pi\right)^{e_{Bp}}}\Delta^{*}\psi_{ref}\right]\nabla\phi\right)
\end{equation}

\end_inset


\begin_inset Formula 
\begin{equation}
\nabla p=\mathbf{j}\times\mathbf{B}
\end{equation}

\end_inset


