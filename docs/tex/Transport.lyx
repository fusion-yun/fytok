\begin_layout Standard
Diamagnetic function
\begin_inset Formula 
\begin{equation}
F=R_{0}B_{0}-\frac{\mu_{0}}{4\pi^{2}}\int_{V}\mathbf{j}\cdot\nabla\theta d^{3}x\equiv R_{0}B_{0}-\frac{\mu_{0}}{2\pi}\int_{S_{\theta}}\mathbf{j}\cdot d\mathbf{S}_{\theta}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
Toroidal magnetic flux coordiantes
\begin_inset Formula 
\begin{equation}
\Phi_{tor}\equiv\int_{S_{\phi}}\mathbf{B}\cdot d\mathbf{S}_{\phi}=\frac{1}{2\pi}\int_{V}\mathbf{B}\cdot\nabla\phi dx^{3}=\frac{1}{2\pi}\int_{V}\frac{F}{R^{2}}dx^{3}
\end{equation}

\end_inset


\begin_inset Formula 
\begin{equation}
d\Phi_{tor}=2\pi qd\psi
\end{equation}

\end_inset


\begin_inset Formula 
\begin{equation}
\Phi_{tor}\left(\psi\right)-\Phi_{tor}\left(\psi_{0}\right)=2\pi\int_{\psi_{0}}^{\psi}qd\psi
\end{equation}

\end_inset


\begin_inset Formula $\rho$
\end_inset

 has the dimensionality of length and represents an effective minor radius
 
\begin_inset Formula 
\begin{equation}
\rho\equiv\sqrt{\frac{\Phi_{tor}}{\pi B_{0}}}
\end{equation}

\end_inset


\begin_inset Formula 
\begin{align*}
\frac{d\rho}{d\psi} & =\frac{1}{2\rho}\frac{1}{\pi B_{0}}\frac{d\Phi_{tor}}{d\psi}\\
 & =\frac{q}{B_{0}\rho}\\
 & =\frac{fV^{\prime}}{B_{0}\sqrt{\frac{\Phi_{tor}}{\pi B_{0}}}}\\
\end{align*}

\end_inset


\end_layout

\begin_layout Subsection
cylindrical coordinates 
\begin_inset Formula $\left(R,\phi,Z\right)$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\mathbf{B}=F\nabla\phi+\frac{\sigma_{Bp}}{\left(2\pi\right)^{e_{Bp}}}\nabla\phi\times\nabla\psi_{ref}
\end{equation}

\end_inset


\begin_inset Formula 
\begin{align}
\nabla\phi & =\frac{1}{R}\hat{\phi}
\end{align}

\end_inset


\begin_inset Formula 
\begin{equation}
\psi_{ref}\equiv-\frac{\sigma_{Bp}}{\left(2\pi\right)^{1-e_{Bp}}}\int\mathbf{B}\cdot d\mathbf{S}_{p}
\end{equation}

\end_inset


\begin_inset Formula 
\begin{align}
\mathbf{A}_{\phi} & =-\frac{\sigma_{Bp}}{\left(2\pi\right)^{e_{Bp}}}\psi_{ref}\nabla\phi\\
A_{\phi} & =-\frac{\sigma_{Bp}}{\left(2\pi\right)^{e_{Bp}}}\frac{\psi_{ref}}{R}
\end{align}

\end_inset


\begin_inset Formula 
\begin{align}
\mathbf{B}_{p} & =\nabla\times\mathbf{A}_{\phi}=\frac{\sigma_{Bp}}{\left(2\pi\right)^{e_{Bp}}}\nabla\phi\times\nabla\psi_{ref}\nonumber \\
B_{\phi} & =\frac{F}{R}
\end{align}

\end_inset


\begin_inset Formula 
\begin{equation}
\Phi_{tor}=\int\mathbf{B}\cdot d\mathbf{S}_{\phi}=\int B_{\phi}dS_{\phi}
\end{equation}

\end_inset


\begin_inset Formula 
\[
dS_{\phi}=\sigma_{Bp}\frac{d\psi_{ref}}{\left|\nabla\psi_{ref}\right|}
\]

\end_inset


\begin_inset Formula 
\begin{align*}
q & =\frac{1}{2\pi}\int\frac{\mathbf{B}\cdot\nabla\phi}{\mathbf{B}\cdot\nabla\theta}d\theta\\
 & =\frac{\sigma_{Bp}}{\left(2\pi\right)^{1-e_{Bp}}}\int\frac{F}{R^{2}}Jd\theta\\
 & =\frac{\sigma_{Bp}}{\left(2\pi\right)^{1-e_{Bp}}}\int\frac{F}{R^{2}}\frac{dl_{p}}{\left|\nabla\psi_{ref}\right|}\\
 & =\frac{\sigma_{Bp}\sigma_{\rho\theta\phi}}{\left(2\pi\right)^{1-e_{Bp}}}\frac{d\Phi_{tor}}{d\psi_{ref}}
\end{align*}

\end_inset

where 
\begin_inset Formula $J^{-1}=\nabla\phi\times\nabla\psi_{ref}$
\end_inset

 
\end_layout

\begin_layout Standard
\begin_inset Formula $(R,\phi,Z)$
\end_inset

 right-handed cylindrical coordinate system: 
\begin_inset Formula 
\[
\mathbf{B}_{p}=\frac{\sigma_{Bp}}{\left(2\pi\right)^{e_{Bp}}}\left\{ \begin{gathered}\frac{1}{R}\frac{\partial\psi_{ref}}{\partial Z}\\
0\\
-\frac{1}{R}\frac{\partial\psi_{ref}}{\partial R}
\end{gathered}
\right\} 
\]

\end_inset


\begin_inset Formula 
\begin{align*}
B_{R} & =\frac{\sigma_{Bp}\sigma_{R\phi Z}}{\left(2\pi\right)^{e_{Bp}}}\frac{1}{R}\frac{\partial\psi_{ref}}{\partial Z}=\frac{1}{2\pi}\frac{1}{R}\frac{\partial\psi_{ref}}{\partial Z}\\
B_{Z} & =-\frac{\sigma_{Bp}\sigma_{R\phi Z}}{\left(2\pi\right)^{e_{Bp}}}\frac{1}{R}\frac{\partial\psi_{ref}}{\partial R}=-\frac{1}{2\pi}\frac{1}{R}\frac{\partial\psi_{ref}}{\partial R}\\
B_{\phi} & =\frac{F}{R}
