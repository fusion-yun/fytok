\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\frac{a\left(\rho\right)\cdot Y\left(\rho,t\right)-b\left(\rho\right)\cdot Y\left(\rho,t-1\right)}{h}+\frac{1}{c\left(\rho\right)}\frac{\partial}{\partial\rho}\left(-d\left(\rho\right)\cdot\frac{\partial Y\left(\rho,t\right)}{\partial\rho}+e\left(\rho\right)\cdot Y\left(\rho,t\right)\right)=f\left(\rho\right)-g\left(\rho\right)\cdot Y\left(\rho,t\right)
\end{equation}

\end_inset

where 
\begin_inset Formula $Y$
\end_inset

 is the function, 
\begin_inset Formula $t$
\end_inset

 is the time and 
\begin_inset Formula $x$
\end_inset

 is the radial coordinate.
\begin_inset Formula 
\begin{equation}
v\left(x_{bnd}\right)\cdot\left.\frac{\partial Y\left(x,t\right)}{\partial x}\right|_{bnd}+u\left(x_{bnd}\right)\cdot Y\left(x_{bnd},t\right)=w\left(x_{bnd}\right)
\end{equation}

\end_inset


\begin_inset Formula 
\[
v\cdot\left.\frac{\partial Y}{\partial x}\right|_{x=bnd}+u\cdot Y=w
\]

\end_inset


\begin_inset Formula 
\[
Y^{\prime}\equiv\frac{\partial Y\left(\rho,t\right)}{\partial\rho}
\]

\end_inset


\begin_inset Formula 
\begin{align*}
\frac{a\cdot Y-b\cdot Y^{-1}}{h}+\frac{1}{c}\frac{\partial}{\partial\rho}\left(-d\cdot Y^{\prime}+e\cdot Y\right) & =f-g\cdot Y\\
ca\cdot Y-cb\cdot Y^{-1}-hd^{\prime}\cdot Y^{\prime}-hd\cdot Y^{\prime\prime}+he^{\prime}\cdot Y+he\cdot Y^{\prime} & =chf-chg\cdot Y
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
Y^{\prime\prime} & =\frac{-d^{\prime}+e}{d}\cdot Y^{\prime}+\frac{ca/h+e^{\prime}+cg}{d}\cdot Y-\frac{c}{d}\left(\frac{b}{h}\cdot Y^{-1}+f\right)
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
A & \equiv\frac{-d^{\prime}+e}{d}\\
B & \equiv\frac{ca/h+e^{\prime}+cg}{d}\\
C & \equiv-\frac{c}{d}\frac{b}{h}\cdot Y^{-1}-\frac{c}{d}f
\end{align*}

\end_inset


\begin_inset Formula 
\[
\begin{cases}
\frac{\partial Y}{\partial\rho} & =Y^{\prime}\\
\frac{\partial Y^{\prime}}{\partial\rho} & =A\cdot Y^{\prime}+B\cdot Y+C
\end{cases}
\]

\end_inset

and
\begin_inset Formula 
\[
v\cdot\left.Y^{\prime}\right|_{x=bnd}+u\cdot Y=w
\]

\end_inset


\end_layout

