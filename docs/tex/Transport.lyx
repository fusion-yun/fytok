\end_inset


\begin_inset Formula 
\begin{align*}
\text{FUN1} & =\frac{\sigma_{\parallel}\mu_{0}\rho^{2}}{F^{2}}\\
\text{d\text{FUN1}} & =\frac{\partial}{\partial\rho}\frac{\sigma_{\parallel}\mu_{0}\rho^{2}}{F^{2}}\\
\text{FUN2} & =\frac{V^{\prime}}{4\pi^{2}}\left\langle \left|\frac{\nabla\rho}{R}\right|^{2}\right\rangle =\frac{V^{\prime}}{4\pi^{2}}g3
\end{align*}

\end_inset


\begin_inset Formula 
