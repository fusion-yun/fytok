\begin_layout Subsection
magnetic flux coordinates 
\begin_inset Formula $\left(\rho,\theta,\phi\right)$
\end_inset


\end_layout

\begin_layout Standard
Intrduce 
\begin_inset Formula $\rho=\rho\left(\psi\right)$
\end_inset

 as a function of poloidal flux 
\begin_inset Formula $\psi$
\end_inset

, and take 
\begin_inset Formula $0<\theta<2\pi$
\end_inset

 to be a poloidal angle, 
\begin_inset Formula $0<\phi<2\pi$
\end_inset

 to be the toroidal angle.
 We have 
\begin_inset Formula 
\begin{align*}
\left|\nabla\phi\right| & =1/R^{2}\\
\nabla\theta\cdot\nabla\phi & =0\\
\nabla\psi\cdot\nabla\phi & =0
\end{align*}

\end_inset

and
\begin_inset Formula 
\begin{equation}
g_{\rho\theta\phi}\equiv\left(\frac{D\left(x,y,z\right)}{D\left(\rho,\theta,\phi\right)}\right)^{2}=\left(\nabla\rho\times\nabla\theta\cdot\nabla\phi\right)^{-2}=J^{2},
\end{equation}

\end_inset


\begin_inset Formula 
\begin{align*}
\sqrt{g}^{-1} & =J^{-1}=\nabla\rho\times\nabla\theta\cdot\nabla\phi\\
 & =\frac{\partial\rho}{\partial Z}\frac{\partial\theta}{\partial R}-\frac{\partial\rho}{\partial R}\frac{\partial\theta}{\partial Z}\\
 & =\rho^{\prime}\left(\frac{\partial\psi}{\partial Z}\frac{\partial\theta}{\partial R}-\frac{\partial\psi}{\partial R}\frac{\partial\theta}{\partial Z}\right)
\end{align*}

\end_inset

is the determinant of the metric tensor, Jacobian 
\begin_inset Formula $J=\sqrt{g}$
\end_inset

,
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
dxdydz=RdRdZd\phi=\sqrt{g}d\rho d\theta d\phi.
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
The volume 
\begin_inset Formula $V$
\end_inset

 with in a flux surface 
\begin_inset Formula $S_{\rho}$
\end_inset

 is given by
\begin_inset Formula 
\begin{align}
V\left(t,\rho\right) & =\int_{V}dV\nonumber \\
 & =\int_{0}^{\rho}d\rho\int_{S_{\rho}}\frac{dS_{\rho}}{\left|\nabla\rho\right|}\nonumber \\
 & =\int_{0}^{\rho}d\rho\int_{0}^{2\pi}d\theta\int_{0}^{2\pi}d\phi\sqrt{g}\nonumber \\
 & =2\pi\int_{0}^{\rho}d\rho\int_{0}^{2\pi}d\theta\sqrt{g}.
\end{align}

\end_inset

The flux surface average of an arbitrary function 
\begin_inset Formula $\alpha\left(\mathbf{r}\right)$
\end_inset

 is denfined
\begin_inset Formula 
\begin{align}
\left\langle \alpha\right\rangle  & \equiv\frac{\partial}{\partial V}\int\alpha dV\\
 & =\int_{0}^{\rho}d\rho\int_{0}^{2\pi}d\theta\int_{0}^{2\pi}d\phi\alpha\sqrt{g}\\
 & =\frac{2\pi}{V^{\prime}}\int_{0}^{2\pi}\alpha\sqrt{g}d\theta\\
\left\langle \alpha\right\rangle  & =\frac{2\pi}{V^{\prime}}\int_{0}^{2\pi}\alpha\sqrt{g}d\theta=\frac{2\pi}{V^{\prime}}\varoint\alpha\frac{R}{\left|\nabla\rho\right|}dl,
\end{align}

\end_inset

where
\begin_inset Formula 
\begin{equation}
V^{\prime}\left(\rho\right)=\frac{\partial V}{\partial\rho}=2\pi\int_{0}^{2\pi}\sqrt{g}d\theta=2\pi\varoint\frac{R}{\left|\nabla\rho\right|}dl.
\end{equation}

\end_inset

The surface area
\begin_inset Formula 
\begin{equation}
S_{\rho}=V^{\prime}\left\langle \left|\nabla\rho\right|\right\rangle 
\end{equation}

\end_inset

We aslo have
\begin_inset Formula 
\begin{equation}
\left\langle f\left(\rho\right)g\left(\rho,\theta\right)\right\rangle =f\left(\rho\right)\left\langle g\left(\rho,\theta\right)\right\rangle 
\end{equation}

\end_inset


\begin_inset Formula 
\begin{align}
\left\langle \nabla\cdot\mathbf{u}\right\rangle  & =\frac{\partial}{\partial V}\left\langle \mathbf{u}\cdot\nabla V\right\rangle =\frac{\partial\Gamma}{\partial V}
\end{align}

\end_inset

The net flux 
\begin_inset Formula $\Gamma$
\end_inset

 of the vector 
\begin_inset Formula $\mathbf{u}$
\end_inset

 through the whole magnetic surface is given by
\begin_inset Formula 
\begin{equation}
\Gamma=\int\nabla\cdot\mathbf{u}dV=\left\langle \mathbf{u}\cdot V\right\rangle =V^{\prime}\left\langle \mathbf{u}\cdot\nabla\rho\right\rangle .
\end{equation}

\end_inset

and the flux density
\begin_inset Formula 
\begin{equation}
\gamma=\frac{\Gamma}{S_{a}}=\frac{\left\langle \mathbf{u}\cdot\nabla\rho\right\rangle }{\left\langle \left|\nabla\rho\right|\right\rangle }
\end{equation}

\end_inset

Considering a function of magnetic surface 
\begin_inset Formula $f\left(\rho\right)$
\end_inset

 descrives a quantity which statisfies the general diffusion equation
\begin_inset Formula 
\begin{equation}
\frac{\partial f}{\partial t}+\left\langle \nabla\cdot\mathbf{u}\right\rangle =\frac{\partial f}{\partial t}+\frac{\partial\Gamma}{\partial V}=S\left(\rho\right).
\end{equation}

\end_inset

The local flux 
\begin_inset Formula $\mathbf{u}$
\end_inset

can be taken into the form
\begin_inset Formula 
\begin{equation}
\mathbf{u}\left(\rho,\theta\right)=f\left(\rho\right)\tilde{\mathbf{v}}\left(\rho,\theta\right)-\tilde{D}\left(\rho,\theta\right)\nabla f\left(\rho\right)
\end{equation}

\end_inset


\begin_inset Formula 
\begin{equation}
\frac{\partial f}{\partial t}=\frac{1}{V^{\prime}}\frac{\partial}{\partial\rho}\left[V^{\prime}\left\langle \left|\nabla\rho\right|\right\rangle \left(\frac{\left\langle \left(\nabla\rho\right)^{2}\right\rangle }{\left\langle \left|\nabla\rho\right|\right\rangle }D\frac{\partial f}{\partial\rho}-vf\right)\right]+S\left(r\right)
\end{equation}

\end_inset

where 
\begin_inset Formula 
\begin{align}
D\left(\rho\right) & =\frac{\left\langle \left(\nabla\rho\right)^{2}\tilde{D}\right\rangle }{\left\langle \left(\nabla\rho\right)^{2}\right\rangle }\\
v & =\frac{\left\langle \nabla\rho\cdot\tilde{\mathbf{v}}\right\rangle }{\left\langle \left|\nabla\rho\right|\right\rangle }
\end{align}

\end_inset


\begin_inset Formula 
\begin{align}
\Gamma\left(\rho\right) & =V^{\prime}\left(\left\langle \left|\nabla\rho\right|\right\rangle vf-\left\langle \left(\nabla\rho\right)^{2}\right\rangle D\frac{\partial f}{\partial\rho}\right)\\
\gamma & =vf-\frac{\left\langle \left(\nabla\rho\right)^{2}\right\rangle }{\left\langle \left|\nabla\rho\right|\right\rangle }D\frac{\partial f}{\partial\rho}
\end{align}

\end_inset


\end_layout

