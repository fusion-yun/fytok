+<?xml version="1.0"?>
+<constants name="transport_solver_numerics_computation_mode" identifier="yes" create_mapping_function="yes">
+<header>Computation mode for this equation</header>
+<ddInstance xpath="/transport_solver_numerics/solver_1d/equation/computation_mode"/>
+
+<int name="static"           description="Equation is not solved, no profile evolution" unique="yes">0</int>
+<int name="interpretative"   description="Equation is not solved, profile is evolved by interpolating from input objects" unique="yes">1</int>
+<int name="predictive"       description="Equation is solved, profile evolves" unique="yes">2</int>
+ 
+</constants>
