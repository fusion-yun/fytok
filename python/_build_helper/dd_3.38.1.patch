diff -ruN 3.38.1-orig/dd_3.38.1/include/equilibrium/equilibrium_gs_deviation.xml 3.38.1/dd_3.38.1/include/equilibrium/equilibrium_gs_deviation.xml
--- 3.38.1-orig/dd_3.38.1/include/equilibrium/equilibrium_gs_deviation.xml	1970-01-01 08:00:00.000000000 +0800
+++ 3.38.1/dd_3.38.1/include/equilibrium/equilibrium_gs_deviation.xml	2023-04-24 08:28:28.261505746 +0800
@@ -0,0 +1,12 @@
+<?xml version="1.0"?>
+<constants name="equilibrium_gs_deviation" identifier="yes" create_mapping_function="yes">
+<header>Expression for calculating the residual deviation between the left and right hand side of the Grad Shafranov equation</header>
+<ddInstance xpath="/equilibrium/time_slice(itime)/convergence/grad_shafranov_deviation_expression"/>
+
+<int name="absolute_gs_difference"                  description="Average absolute difference of the Grad-Shafranov equation, &lt;|Del* psi - j_tor*R|&gt;, averaged over the plasma poloidal cross-section" unique="yes">1</int>
+<int name="root_mean_square_gs_difference"          description="Root mean square difference of the Grad-Shafranov equation, sqrt(&lt;(Del* psi - j_tor*R)^2 &gt;), averaged over the plasma poloidal cross-section" unique="yes">2</int>
+<int name="max_absolute_psi_residual"               description="Maximum absolute difference over the plasma poloidal cross-section of the poloidal flux between the current and preceding iteration, on fixed grid points" unique="yes">3</int>
+<int name="max_absolute_gs_difference_norm"         description="Maximum absolute difference of the Grad-Shafranov equation, normalised, max(|Del* psi - j_tor*R|) / max(|Del* psi|), over the plasma poloidal cross-section" unique="yes">4</int>
+<int name="max_root_mean_square_gs_difference_norm" description="Root maximum square difference of the Grad-Shafranov equation, normalised, sqrt(max((Del* psi - j_tor*R)^2) / max((Del* psi)^2)), over the plasma poloidal cross-section" unique="yes">5</int>
+
+</constants>
diff -ruN 3.38.1-orig/dd_3.38.1/include/transport_solver_numerics/transport_solver_numerics_bc_type.xml 3.38.1/dd_3.38.1/include/transport_solver_numerics/transport_solver_numerics_bc_type.xml
